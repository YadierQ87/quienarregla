<p id="notice"><%= notice %></p>

<h1>Encuesta</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Fecha</th>
      <th>Pregunta</th>
      <th>Puntuacion</th>
      <th>Aspectoevaluar</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @encuesta.each do |encuestum| %>
      <tr>
        <td><%= encuestum.nombre %></td>
        <td><%= encuestum.fecha %></td>
        <td><%= encuestum.pregunta %></td>
        <td><%= encuestum.puntuacion %></td>
        <td><%= encuestum.aspectoEvaluar %></td>
        <td><%= link_to 'Show', encuestum %></td>
        <td><%= link_to 'Edit', edit_encuestum_path(encuestum) %></td>
        <td><%= link_to 'Destroy', encuestum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Encuestum', new_encuestum_path %>
